import math

def volume(h, r):
        volume = (1/3) * math.pi * r**2 * h
        return volume

# Getting user inputs for height and radius
height = float(input("Enter the height of the cone: "))
radius = float(input("Enter the radius of the cone: "))

# returning the volume of the cone
print("The volume of the cone is:", volume(height, radius))
